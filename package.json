{
  "name": "eslint-config-springworks",
  "version": "3.2.0",
  "description": "ESLint configurations used by Springworks",
  "main": "index.js",
  "author": "Springworks",
  "license": "MIT",
  "scripts": {
    "lint": "eslint -c index-config.json --max-warnings 0 examples/example.js",
    "lint-es6": "eslint -c babel-config.json --max-warnings 0 examples/example-es6.js",
    "test": "npm run lint && npm run lint-es6"
  },
  "peerDependencies": {
    "eslint": "^1.4.3",
    "eslint-plugin-mocha": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "4.1.3",
    "eslint": "1.7.0"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Springworks/eslint-config-springworks.git"
  },
  "bugs": {
    "url": "https://github.com/Springworks/eslint-config-springworks/issues"
  },
  "homepage": "https://github.com/Springworks/eslint-config-springworks#readme"
}
